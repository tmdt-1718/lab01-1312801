  <%for painting in @gallery.paintings.each %>
  <div class="col-md-4">
<div class="card" >
    <div class="card-body">
    <div class="painting">
      <%= image_tag painting.image_url(:thumb).to_s %>
      <div class="image_placeholder"></div>
      <div class="name"><%= painting.name %></div>
      <%= link_to "Add",new_gallery_path%> |
      <%= link_to "Delete",@gallery,:confirm=>"Are you sure?",method: :delete%> |
      <%= link_to "View Album",galleries_path%>
    </div>
  </div>
  </div>
</div>
  <% end %>
